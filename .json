{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build TaskFlow",
      "type": "shell",
      "command": "C:/MinGW/bin/g++.exe",
      "args": [
        "-g",
        "main.cpp",
        "-o",
        "taskflow.exe"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ],
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "grunt": "^1.6.1",
    "grunt-cli": "^1.4.3"
  }
}
